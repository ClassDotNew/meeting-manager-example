<h1><a href="/meetings">All meetings</a></h1>
<div ng-app="app">
<div ng-controller="meetingsCtrl" ng-init="setup()">
  <h3>Create a new meeting:</h3>
   <form>
     <div>
       <label>Name:</label>
       <input ng-model="newMeetingName">
     </div>
     <div>
       <label>Address:</label>
       <input ng-model="newMeetingAddress">
     </div>
     <div>
       <label>Start time:</label>
       <input ng-model="newMeetingStartTime" type="datetime-local">
     </div>
     <div>
       <label>End time:</label>
       <input ng-model="newMeetingEndTime" type="datetime-local">
     </div>
     <div>
       <label>Notes</label>
       <textarea ng-model="newMeetingNotes" cols="30" rows="10"></textarea>
     </div>
     <input type="submit" ng-click="createMeeting(newMeetingName,newMeetingAddress,newMeetingStartTime,newMeetingEndTime,newMeetingNotes)">
   </form>
Search here for the name of your meeting
<input ng-model="meetingSearch" list="names"> <br>
<datalist id="names">
  <option ng-repeat="meeting in meetings">
  {{meeting.name}}
  </option>
</datalist>
<button ng-click="sortByAttribute()">Sort by Date</button>
  <div ng-repeat="meeting in meetings | filter: { name: meetingSearch } | orderBy:attribute:descending">
    <h3><a href="/meetings/{{meeting.id }}">{{ meeting.name }} </a></h3>
    <p>
      Tags:
      <div ng-repeat="tag in meeting.tags">
        <a href="/meetings?tag={{ tag.name }}">{{ tag.name }}</a>
      </div>
    </p>

  <p>Address: {{ meeting.address }}</p>
  <p>Start time: {{ meeting.startTime }}</p>
  <p>End time: {{ meeting.endTime }}</p>
  </div>
</div>
  {{1 + 1}}

</div>

